{
  "name": "@mapgis/boot-docs",
  "version": "0.0.2",
  "private": true,
  "author": "MapGIS",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "lint": "prettier --write 'docs/**/*.md'"
  },
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-medium-zoom": "^1.8.2",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "vuepress": "^1.5.2",
    "vuepress-plugin-viewer": "^1.0.0"
  },
  "bugs": {
    "url": "https://github.com/mapgis/mapgis-boot-docs/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "mapgis",
    "mapgis-boot",
    "map"
  ],
  "lint-staged": {
    "*.{js,json,css,md,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapgis/mapgis-boot-docs.git"
  }
}
